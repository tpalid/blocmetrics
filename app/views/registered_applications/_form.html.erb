<%= form_for [registered_application] do |f| %>
    <div class='form-group'>
        <%= f.label :name %></br>
        <%= f.text_field :name %>
    </div>
    
    <div class="form-group">
        <%= f.label :url %></br>
        <%= f.text_field :url %>
    </div>
    
    <div class="form-group">
        <%= f.submit "Save", class: 'btn btn-success' %>
    </div>
<% end %>
